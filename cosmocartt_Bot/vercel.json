{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/products(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/categories",
            "destination": "/api/index.py"
        },
        {
            "source": "/subcategories",
            "destination": "/api/index.py"
        },
        {
            "source": "/orders(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/cart(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/send-otp",
            "destination": "/api/index.py"
        },
        {
            "source": "/verify-otp",
            "destination": "/api/index.py"
        },
        {
            "source": "/static/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/docs",
            "destination": "/api/index.py"
        },
        {
            "source": "/openapi.json",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}